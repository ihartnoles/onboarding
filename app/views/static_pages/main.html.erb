   <section id="main" class="login-register">
  <header class="page-header">
    <div class="container">
      <h3 class="title">FAU Student Onboarding Portal <br> Please Choose your Login Method</h3>
    </div>  
  </header>
  <div class="container">
    <div class="row">
      <div class="content col-sm-12 col-md-12">
    <div class="row filter-elements hidden">
      <div class="col-sm-6  box login">
      <div class="info-box col-sm-6 col-md-6">
        
        <header class="page-header">
            <div class="container">
        <h3 class="title col-sm-6 col-md-6">Method 1:<br> Use your FAU NetID</h3>
            </div>
        </header>

        <p class="descriptions">If you have been assigned an FAU NetID you should login with this method.</p>
        <button id="btnNetID" class="btn btn-danger filter-buttons">Login with NETID</button>
      </div>
      </div>

      <div class="col-sm-6 col-md-6 box login">
     

         <%= form_for @login, :url => do_manual_login_path, :method => "post", :html => {:class => 'form-box login-form form-validator bv-form'}  do |f| %>
          
          <header class="page-header">
            <div class="container">
            <h3 class="title col-sm-6 col-md-6">Method 2: <br> Use your Admissions Credentials</h3>
            </div>
          </header>
                  <p>If you do NOT have an FAU NetID yet you can login with the same credentials you set up when applying to FAU</p>
            

            <div class="form-group has-feedback">
              <label>Email Address: <span class="required">*</span></label>
              <%#= f.label :email %><br>
              <%#= f.text_field :email, class: 'form-control', 'type'=>'email',  'data-bv-emailaddress-message' => 'The input is not a valid email address', 'data-bv-field'=>'email'%>

               <%= text_field_tag :email, params[:email] , class: 'form-control', 'required' => '', 'type'=>'email',  'data-bv-emailaddress-message' => 'The input is not a valid email address', 'data-bv-field'=>'email'%>
              <i class="form-control-feedback" data-bv-icon-for="email" style="display: none;"></i>
                        <small class="help-block" data-bv-validator="emailAddress" data-bv-for="email" data-bv-result="NOT_VALIDATED" style="display: none;">The input is not a valid email address</small><small class="help-block" data-bv-validator="notEmpty" data-bv-for="email" data-bv-result="NOT_VALIDATED" style="display: none;">Please enter a value</small>
              </div>

            <div class="form-group has-feedback">
              <label>Password: <span class="required">*</span></label>
              
             <%#= f.text_field :password, class: 'form-control'  %>
             <%= text_field_tag :password, params[:password] , class: 'form-control', 'required' => '','type'=>'password', 'data-bv-field'=>'password'%>

             <i class="form-control-feedback" data-bv-icon-for="password" style="display: none;"></i>
                        <small class="help-block" data-bv-validator="notEmpty" data-bv-for="password" data-bv-result="NOT_VALIDATED" style="display: none;">Please enter a value</small>

            </div>
            <div class="actions">
              <%= f.submit  'Login', :class => "btn btn-danger" %>
            </div>
        <% end %>
      </div>

     

     
    </div>
      </div>
    </div>
  </div><!-- .container -->
</section><!-- #main -->

